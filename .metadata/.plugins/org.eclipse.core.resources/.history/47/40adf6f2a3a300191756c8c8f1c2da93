
/*
 * 실수형 데이터 타입
   float : 4byte (부호비트1, 지수부8, 가수부 23)
   double : 8byte (부호비트1, 지수부11, 가수부 52)
*/
package chapter2;

public class A03_Double {
	public static void main(String[] args) {
		double dnum = 3.14;
		float fnum = 3.14f; //float는 f를 붙여한다
		System.out.println(dnum);
		System.out.println(fnum);
		
	}

}
